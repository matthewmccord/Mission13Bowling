@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-teamName="" class="btn btn-secondary btn-sm">Home</a>
    <br />
    @foreach (string t in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-teamName="@t" class="btn btn-sm @(ViewBag.SelectedTeam == t ? "btn-primary":"btn-secondary")">@t</a>
        <br />
    }


</div>
